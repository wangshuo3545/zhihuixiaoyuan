{
	"pages": [{
		"path": "pages/index/index",
		"style": {
			"navigationBarTitleText": "云晨科技-智慧校园v3",
			"mp-alipay": {
				"defaultTitle": ""
			}
		}
	},
	{
		"path" : "views/Login/index",
		"style" : 
		{
			"navigationBarTitleText" : ""
		}
	},
	{
		"path" : "views/Login/Register",
		"style" : 
		{
			"navigationBarTitleText" : ""
		}
	}],
	"subPackages": [{
			"root": "demo-pages",
			"pages": [{
					"path": "graphic-card/index",
					"style": {
						"navigationBarTitleText": "发布详情"
					}
	
				}
			]
	}],
	"preloadRule": {
		"pages/index/index": {
			"network": "all",
			"packages": ["demo-pages"]
		}
	},
	"globalStyle": {
		"navigationStyle": "custom",
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "云晨科技-智慧校园v3",
		"navigationBarBackgroundColor": "#F7F7F7",
		"backgroundColor": "#F7F7F7",
		"mp-alipay": {
			"transparentTitle": "always",
			"titlePenetrate": "YES",
			"defaultTitle": "",
			"titleBarColor": "#FFFFFF",
			"backgroundColor": "#FFFFFF",
			"frontColor": "#000000",
			"allowsBounceVertical": "NO"
		}
		
	},
	"easycom": {
	    "custom": {
	      "^tn-(.*)-(item|group)$": "node_modules/@tuniao/tnui-vue3-uniapp/components/$1/src/$1-$2.vue",
	      "^tn-(.*)": "node_modules/@tuniao/tnui-vue3-uniapp/components/$1/src/$1.vue"
	    }
	  }
}